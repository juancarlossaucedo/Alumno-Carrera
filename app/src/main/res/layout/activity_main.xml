<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".vistas.MainActivity">

    <Button
            android:text="Alumnos"
            android:layout_width="254dp"
            android:layout_height="50dp"
            android:id="@+id/btnContactos"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="208dp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <Button
            android:text="Carreras"
            android:layout_width="254dp"
            android:layout_height="50dp"
            android:id="@+id/btnCarrera"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="280dp"
            app:layout_constraintHorizontal_bias="0.498"
    />

</androidx.constraintlayout.widget.ConstraintLayout>